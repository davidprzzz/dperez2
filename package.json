    "url": "git+https://github.com/davidprzzz/dperez2.git"
  },
  "template": {
    "name": "phaser-project-template-es6",
    "url": "https://github.com/davidprzzz/dperez2.git",
    "author": "David Perez (https://github.com/davidprzzz)"
  },
